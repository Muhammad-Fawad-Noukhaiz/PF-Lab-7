#include <stdio.h>

void findDuplicates(int arr[], int n) {
    int i;

    printf("The repeating elements are: ");
    
    for (i = 0; i < n; i++) {
        int index = arr[i] % n;
        arr[index] += n;
    }

    for (i = 0; i < n; i++) {
        if ((arr[i] / n) > 1) {  // If value is greater than 1*n, it means the element appeared more than once
            printf("%d ", i);
        }
    }
    
    printf("\n");
}

int main() {
    int n;

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter %d elements:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    findDuplicates(arr, n);

    return 0;
}
